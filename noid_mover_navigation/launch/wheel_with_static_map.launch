<?xml version="1.0"?>
<launch>
  <arg name="RUN_BRINGUP" default="true" />

  <!--- Run bringup -->
  <include if="$(arg RUN_BRINGUP)"
           file="$(find noid_mover_navigation)/launch/wheel_bringup.launch" />

  <!-- Run map navigation -->
  <arg name="map_file" default="$(find noid_mover_navigation)/maps/map.yaml"/>
  <arg name="costmap_file" default="$(find noid_mover_navigation)/maps/map.yaml"/>
  <include file="$(find noid_mover_navigation)/launch/static_map_navigation.launch">
    <arg name="map_file" value="$(arg map_file)" />
    <arg name="costmap_file" value="$(arg costmap_file)"/>
  </include>

</launch>
